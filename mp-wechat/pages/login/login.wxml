
<view class="box">
<view class="login">
<van-cell-group>
  <van-field
    value="{{ mobile }}"
    required
    clearable
    label="手机号"
    placeholder="手机号"
  />

  <van-field
    value="{{ password }}"
    type="password"
    label="密码"
    placeholder="请输入密码"
    required
    border="{{ false }}"
  />
</van-cell-group>
<view class="my-login-wrap">
<van-button  size="small" type="primary">登录</van-button>
<van-button style="margin-left:20rpx;" plain size="small" type="primary" bind:click="goReg">快速注册</van-button>
</view>

</view>

</view>
