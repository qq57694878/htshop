.my-list {
  padding: 20rpx;
  overflow: hidden;
  background-color: #fff;
  font-size: 32rpx;
  color: #fff;
}

.my-list-item {
  margin-bottom: 20rpx;
  background-color: #fff;
  width: 100%;
  height: 360rpx;
  position: relative;
}

.my-card-bg {
  background-color: #fff;
  width: 100%;
  height: 360rpx;
  border-radius: 6px;
}

.my-card-icon-wp {
  position: absolute;
  left: 20rpx;
  top: 40rpx;
}

.my-card-icon {
  font-size: 50rpx !important;
}

.my-right-icon {
  font-size: 36rpx !important;
}

.my-card-name {
  position: absolute;
  left: 80rpx;
  top: 20rpx;
}

.my-total {
  position: absolute;
  left: 80rpx;
  top: 66rpx;
  font-size: 28rpx;
}

.my-rest {
  position: absolute;
  left: 240rpx;
  top: 190rpx;
  color: #ff495e;
}

.my-rest-1 {
}

.my-rest-2 {
  font-size: 50rpx;
  font-weight: bold;
}

.my-rest-3 {
}

.my-valid-time {
  position: absolute;
  right: 10rpx;
  top: 66rpx;
  font-size: 28rpx;
}

.my-right-icon-wp {
  position: absolute;
  right: 20rpx;
  top: 16rpx;
}


.my-icon-ewm-wp {
  position: absolute;
  right: 20rpx;
  top: 190rpx;
}

.my-icon-ewm {
  font-size: 50rpx!important;

}

