<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kulongtai.mpstore.mapper.SkuMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.kulongtai.mpstore.entity.Sku">
        <id column="sku_id" property="skuId" />
        <result column="sku_name" property="skuName" />
        <result column="sku_price" property="skuPrice" />
        <result column="frequency" property="frequency" />
        <result column="sku_content" property="skuContent" />
        <result column="sort" property="sort" />
        <result column="is_no_sale" property="isNoSale" />
        <result column="sku_status" property="skuStatus" />
        <result column="catagory" property="catagory" />
        <result column="valid_month" property="validMonth" />
        <result column="del_flag" property="delFlag" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        sku_id, sku_name, sku_price, frequency, sku_content, sort, is_no_sale, sku_status, catagory, valid_month, del_flag, create_time, update_time
    </sql>

</mapper>
